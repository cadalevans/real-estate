<div class="content-area submit-property" style="background-color: #FCFCFC;">
    <div class="container">
        <div class="clearfix">
            <div class="wizard-container">
                <div class="wizard-card ct-wizard-orange" id="wizardProperty">
                    {{ form_start(form, {'attr': {'class': 'form-horizontal'}}) }}
                    {{ form_errors(form) }}

                    <!-- Basic Information -->

                        <h4 class="info-text"> Let's start with the basic information (with validation)</h4>
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="col-sm-12">
                            <div class="form-group">
                                {{ form_label(form.title) }}
                                {{ form_widget(form.title, {'attr': {'class': 'form-control', 'placeholder': 'Super villa ...'}}) }}
                                {{ form_errors(form.title) }}
                            </div>
                            <div class="form-group">
                                {{ form_label(form.price) }}
                                {{ form_widget(form.price, {'attr': {'class': 'form-control', 'placeholder': '3330000'}}) }}
                                {{ form_errors(form.price) }}
                            </div>
                            <div class="form-group">
                                {{ form_label(form.area) }}
                                {{ form_widget(form.area, {'attr': {'class': 'form-control', 'placeholder': 'Area'}}) }}
                                {{ form_errors(form.area) }}
                            </div>
                            <div class="form-group">
                                {{ form_label(form.address.street) }}
                                {{ form_widget(form.address.street, {'attr': {'class': 'form-control', 'placeholder': 'Address'}}) }}
                                {{ form_errors(form.address.street) }}
                            </div>
                        </div>
                    </div>

                    <!-- Property Description and Location -->
                    <h4 class="info-text"> How much your Property is Beautiful? </h4>
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="col-sm-12">
                                <div class="form-group">
                                    {{ form_label(form.description) }}
                                    {{ form_widget(form.description, {'attr': {'class': 'form-control', 'placeholder': 'Property Description'}}) }}
                                    {{ form_errors(form.description) }}
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-12">
                            <div class="col-sm-3">
                                <div class="form-group">
                                    {{ form_label(form.address.state) }}
                                    {{ form_widget(form.address.state, {'attr': {'class': 'form-control selectpicker', 'data-live-search': 'true', 'data-live-search-style': 'begins', 'title': 'Select your state'}}) }}
                                    {{ form_errors(form.address.state) }}
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="form-group">
                                    {{ form_label(form.address.city) }}
                                    {{ form_widget(form.address.city, {'attr': {'class': 'form-control selectpicker', 'data-live-search': 'true', 'data-live-search-style': 'begins', 'title': 'Select your city'}}) }}
                                    {{ form_errors(form.address.city) }}
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="form-group">
                                    {{ form_label(form.address.country) }}
                                    {{ form_widget(form.address.country, {'attr': {'class': 'form-control selectpicker show-tick', 'title': 'Select your country'}}) }}
                                    {{ form_errors(form.address.country) }}
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="form-group">
                                    {{ form_label(form.address.postalCode) }}
                                    {{ form_widget(form.address.postalCode, {'attr': {'class': 'form-control', 'placeholder': 'Postal Code'}}) }}
                                    {{ form_errors(form.address.postalCode) }}
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-12 padding-top-15">
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label for="property-geo">Min bed :</label>
                                    <input type="text" class="span2" value="" data-slider-min="0"
                                           data-slider-max="600" data-slider-step="5"
                                           data-slider-value="[250,450]" id="min-bed" ><br />
                                    <b class="pull-left color">1</b>
                                    <b class="pull-right color">120</b>
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label for="price-range">Min baths :</label>
                                    <input type="text" class="span2" value="" data-slider-min="0"
                                           data-slider-max="600" data-slider-step="5"
                                           data-slider-value="[250,450]" id="min-baths" ><br />
                                    <b class="pull-left color">1</b>
                                    <b class="pull-right color">120</b>
                                </div>
                            </div>
                        </div>

                        <br>
                    </div>

                    <!-- Images and Videos -->
                    <h4 class="info-text">Give us some images and videos</h4>
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group">
                                {{ form_label(form.image) }}
                                {{ form_widget(form.image, {'attr': {'class': 'form-control', 'placeholder': 'image add'}}) }}
                                {{ form_errors(form.image) }}
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label for="property-video">Property video:</label>
                                <input class="form-control" placeholder="http://www.youtube.com, http://vimeo.com" name="property_video" type="text">
                            </div>
                        </div>
                    </div>

                    <!-- Agreement -->
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="form-group">
                                <div class="checkbox">
                                    <label>
                                        <input type="checkbox" id="agree-terms"> I agree to the terms and conditions
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Submit Button -->
                    <div class="row">
                        <div class="col-sm-12">
                            <button class="btn btn-primary">Save</button>
                        </div>
                    </div>

                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </div>
</div>
